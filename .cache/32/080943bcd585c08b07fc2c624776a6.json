ents\\_infos_bar.scss":"@use'../variaveis';\r\n\r\n\r\n.infos-bar{\r\n    padding: 16px 24px;\r\n    background-color: variaveis.$textColor;\r\n    display: flex;\r\n    justify-content: space-between;\r\n\r\n    &__infos{\r\n        display: flex;\r\n        gap: 40px;\r\n\r\n        &__item{\r\n            font-size: 14px;\r\n            color:variaveis.$secundaryTextColor;\r\n\r\n            b{\r\n                margin-bottom: 5px;\r\n                font-size: 18px;\r\n                display: block;\r\n                color: variaveis.$secundaryTextColor;\r\n            }\r\n        }\r\n    }\r\n    @media (max-width: 640px) {\r\n        display: block;\r\n\r\n        &__infos{\r\n            display: block;\r\n\r\n            &__item{\r\n                margin-bottom: 24px;\r\n            }\r\n        }\r\n        \r\n    }\r\n    @media (max-width: 1024px) {\r\n            &__infos{\r\n                gap: 16px;\r\n            }\r\n    }\r\n}\r\n\r\n","styles\\components\\_buttons.scss":"@use'../variaveis';\r\n\r\n.button{\r\n    padding: 12px;\r\n    font-size: 18px;\r\n    font-weight: bold;\r\n    text-decoration: none;\r\n    text-align: center;\r\n    white-space: nowrap;\r\n\r\n    &--primary{\r\n        color: white;\r\n        background-color: variaveis.$roxo;\r\n    }\r\n\r\n    @media (max-width:640px) {\r\n        display: block;\r\n        width: 100%;        \r\n    }\r\n    @media (max-width:1024px) {\r\n        display: flex;\r\n        align-items: center;\r\n    }\r\n    }","styles\\_event.scss":"@use 'variaveis';\r\n\r\n.event{\r\n    padding: 80 0;\r\n    \r\n    &--frontend{\r\n        background-image: linear-gradient(variaveis.$blue1, variaveis.$blue2);\r\n    }\r\n\r\n    &--ux-ui{\r\n        background-image: linear-gradient(variaveis.$blue2, variaveis.$pink);\r\n    }\r\n\r\n    &--data-science{\r\n        background-image: linear-gradient(variaveis.$pink, variaveis.$yellow);\r\n    }\r\n\r\n    &--backend{\r\n        background-im